@report{pichMutationalFootprintsCancer2019,
  title = {The Mutational Footprints of Cancer Therapies},
  author = {Pich, Oriol and Muiños, Ferran and Lolkema, Martijn Paul and Steeghs, Neeltje and Gonzalez-Perez, Abel and Lopez-Bigas, Nuria},
  date = {2019-06-27},
  institution = {{Genomics}},
  doi = {10.1101/683268},
  url = {http://biorxiv.org/lookup/doi/10.1101/683268},
  urldate = {2020-09-06},
  abstract = {Some cancer therapies damage DNA and cause mutations both in cancer and healthy cells of the patient             1             . These therapy-induced mutations may underlie some of the long-term and late side effects of the treatment, such as mental disabilities, organ toxicities and secondary neoplasms. Currently we ignore the mutation pattern and burden caused by different cancer treatments. Here we identify mutational signatures, or footprints of six widely-used anti-cancer therapies with the study of whole-genomes from more than 3500 metastatic tumors originated in different organs. These include previously known and new mutational signatures generated by platinum-based drugs, and a novel signature of treatment with nucleoside metabolic inhibitors. Exploiting these mutational footprints, we estimate the contribution of different treatments to the mutation burden of tumors and their risk of causing coding and likely driver mutations in the genome. In summary, the mutational footprints identified here open a window to precisely appraise the mutational risk of different cancer therapies to understand their late side effects.},
  annotation = {00000},
  file = {/Users/wsx/Nutstore Files/zotero-tablet/pich_et_al_2019_the_mutational_footprints_of_cancer_therapies.pdf},
  langid = {english},
  type = {preprint}
}

