@report{kimNetworkbasedApproachesElucidate2019,
  title = {Network-Based Approaches Elucidate Differences within {{APOBEC}} and Clock-like Signatures in Breast Cancer},
  author = {Kim, Yoo-Ah and Wojtowicz, Damian and Basso, Rebecca Sarto and Sason, Itay and Robinson, Welles and Hochbaum, Dorit S. and Leiserson, Mark D.M. and Sharan, Roded and Vandin, Fabio and Przytycka, Teresa M.},
