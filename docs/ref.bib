  options = {useprefix=true}
}

@article{martypykeEvolutionaryPressureMHC2018,
  title = {Evolutionary {{Pressure}} against {{MHC Class II Binding Cancer Mutations}}},
  author = {Marty Pyke, Rachel and Thompson, Wesley Kurt and Salem, Rany M. and Font-Burgada, Joan and Zanetti, Maurizio and Carter, Hannah},
  date = {2018-10},
  journaltitle = {Cell},
