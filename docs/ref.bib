  title = {Genetic Diversity of Tumors with Mismatch Repair Deficiency Influences Anti–{{PD}}-1 Immunotherapy Response},
  author = {Mandal, Rajarsi and Samstein, Robert M. and Lee, Ken-Wing and Havel, Jonathan J. and Wang, Hao and Krishna, Chirag and Sabio, Erich Y. and Makarov, Vladimir and Kuo, Fengshen and Blecua, Pedro and Ramaswamy, Apoorva T. and Durham, Jennifer N. and Bartlett, Bjarne and Ma, Xiaoxiao and Srivastava, Raghvendra and Middha, Sumit and Zehir, Ahmet and Hechtman, Jaclyn F. and Morris, Luc GT and Weinhold, Nils and Riaz, Nadeem and Le, Dung T. and Diaz, Luis A. and Chan, Timothy A.},
