@article{rizviMolecularDeterminantsResponse2018,
  title = {Molecular {{Determinants}} of {{Response}} to {{Anti}}–{{Programmed Cell Death}} ({{PD}})-1 and {{Anti}}–{{Programmed Death}}-{{Ligand}} 1 ({{PD}}-{{L1}}) {{Blockade}} in {{Patients With Non}}–{{Small}}-{{Cell Lung Cancer Profiled With Targeted Next}}-{{Generation Sequencing}}},
  author = {Rizvi, Hira and Sanchez-Vega, Francisco and La, Konnor and Chatila, Walid and Jonsson, Philip and Halpenny, Darragh and Plodkowski, Andrew and Long, Niamh and Sauter, Jennifer L. and Rekhtman, Natasha and Hollmann, Travis and Schalper, Kurt A. and Gainor, Justin F. and Shen, Ronglai and Ni, Ai and Arbour, Kathryn C. and Merghoub, Taha and Wolchok, Jedd and Snyder, Alexandra and Chaft, Jamie E. and Kris, Mark G. and Rudin, Charles M. and Socci, Nicholas D. and Berger, Michael F. and Taylor, Barry S. and Zehir, Ahmet and Solit, David B. and Arcila, Maria E. and Ladanyi, Marc and Riely, Gregory J. and Schultz, Nikolaus and Hellmann, Matthew D.},
